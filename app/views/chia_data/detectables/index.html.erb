<h1>Listing detectables</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Pretty name</th>
      <th>Description</th>
      <th>Chia versions</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @detectables.each do |detectable| %>
      <tr>
        <td><%= detectable.name %></td>
        <td><%= detectable.pretty_name %></td>
        <td><%= detectable.description %></td>
        <td><%= detectable.chia_version.name %></td>
        <td><%= link_to 'Show', [:chia_data, detectable] %></td>
        <td><%= link_to 'Edit', edit_chia_data_detectable_path(detectable) %></td>
        <td><%= link_to 'Destroy', [:chia_data, detectable], method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Detectable', new_chia_data_detectable_path %>
