<%= simple_form_for([:chia_data, @detectable]) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :name %>
    <%= f.input :pretty_name %>
    <%= f.input :description %>
    <%= f.input :dtype, collection: States::DetectableType.possibleStates %>
    <%= f.input :cellroti_id %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
