<%= simple_form_for([:chia_data, @detectable]) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :name %>
    <%= f.input :pretty_name %>
    <%= f.input :description %>
    <%= f.input :chia_detectable_id %>
    <%= f.hidden_field :chia_version_id, value: @chia_version.id %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
